<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boye.dao.ProvideDao">
    
    <select id="getProvideByCode" resultType="provideInfo">
        select * from provide_info where provide_code = #{code} and delete_flag = 0
