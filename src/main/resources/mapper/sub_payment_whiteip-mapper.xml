<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boye.dao.SubPaymentWhiteIpDao">

 	<select id="getSubPaymentWhiteIpByShopId" resultType="SubPaymentWhiteIp">
        select *
		from sub_payment_whiteip 
		where delete_flag=0 and shop_id=#{shop_id}
    </select>   
</mapper>